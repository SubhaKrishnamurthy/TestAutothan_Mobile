<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme --> <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->  <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->   <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- title of the document -->
        <documentTitle>Automation Dashboard</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>AutoBahn Automation Report_Mobile_Android SME</reportName>

        <!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
        <dateFormat>dd-MM-yyyy</dateFormat>

        <!-- global time format override -->   <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>
		
		<testViewChartLocation>bottom</testViewChartLocation>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
        $(document).ready(function() {
        $('.brand-logo').css('display','none');
		$('.report-name').css('padding-left','10px');
        });
      ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

      ]]>
        </styles>
    </configuration>
</extentreports>